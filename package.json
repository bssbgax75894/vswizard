{
    "name": "vswizard",
    "displayName": "Vswizard",
    "description": "Local codex tools using WizardCoder",
    "repository": {
        "type": "git",
        "url": "https://github.com/odeep-co/vswizard"
    },
    "license": "MIT",
    "publisher": "odeep-co",
    "version": "0.1.0",
    "icon": "assets/Logo.png",
    "engines": {
        "vscode": "^1.65.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Code Completion",
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [],
        "menus": {},
        "keybindings": [],
        "configuration": []
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.4",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "@types/vscode": "^1.65.0",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "@typescript-eslint/parser": "^5.1.0",
        "@vscode/test-electron": "^1.6.2",
        "eslint": "^8.1.0",
        "glob": "^7.1.7",
        "mocha": "^9.1.3",
        "typescript": "^4.4.4"
    },
    "dependencies": {
        "axios": "^0.26.1"
    }
}
